{
  "expo": {
    "name": "SecureDo",
    "slug": "secure-do",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_bg_removed.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "hideNavigationBar": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_bg_removed.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/logo_bg_removed.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo_bg_removed.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "navigationBar": {
      "visible": "immersive"
    },
    "androidNavigationBar": {
      "visible": "immersive"
    },
    "androidStatusBar": {
      "translucent": true,
      "backgroundColor": "transparent",
      "barStyle": "light-content"
    },
    "splash": {
      "image": "./assets/logo_bg_removed.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    }
  }
}

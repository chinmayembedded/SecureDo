{
  "expo": {
    "name": "SecureDo",
    "slug": "secure-do",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_bg_removed.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.securedo.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_bg_removed.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [],
      "allowBackup": true,
      "softwareKeyboardLayoutMode": "pan"
    },
    
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/logo_bg_removed.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo_bg_removed.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
            "enableProguardInReleaseBuilds": true,
            "extraProguardRules": "-keep class com.google.android.material.** { *; }",
            "enableShrinkResources": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "androidStatusBar": {
      "translucent": true,
      "barStyle": "light-content",
      "backgroundColor": "#00000088"
    },
    "splash": {
      "image": "./assets/logo_bg_removed.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5d7fb7d7-8a49-4a4e-97d7-b045e7b59a0c"
      }
    },
    "owner": "chinembedded"
  }
}
